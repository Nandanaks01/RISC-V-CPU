00402983  // lw   x19, 4(x0)     ; x19 = mem[4] = 10
00802A03  // lw   x20, 8(x0)     ; x20 = mem[8] = 15

// Branch test
00208A63  // beq  x1, x2, skip   ; if (x1 == x2) skip (not taken)
00500A93  // addi x21, x0, 5     ; x21 = 5 (executed)

// skip:
001B0B13  // addi x22, x22, 1    ; x22 = x22 + 1

// Branch taken test
00000B93  // addi x23, x0, 0     ; x23 = 0
00100C13  // addi x24, x0, 1     ; x24 = 1
018B8463  // beq  x23, x24, end  ; if (x23 == x24) end (not taken)
00200C93  // addi x25, x0, 2     ; x25 = 2 (executed)

