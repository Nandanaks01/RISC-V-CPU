002081B3  // add  x3, x1, x2     ; x3 = x1 + x2 = 15
40208233  // sub  x4, x1, x2     ; x4 = x1 - x2 = -5
002092B3  // sll  x5, x1, x2     ; x5 = x1 << (x2 & 0x1F) = 5120
0020A333  // slt  x6, x1, x2     ; x6 = (x1 < x2) = 1
0020B3B3  // sltu x7, x1, x2     ; x7 = (x1 < x2 unsigned) = 1

