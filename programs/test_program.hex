0020C413  // xor  x8, x1, x2     ; x8 = x1 ^ x2 = 15
0020D493  // srl  x9, x1, x2     ; x9 = x1 >> (x2 & 0x1F) = 0
0020E513  // or   x10, x1, x2    ; x10 = x1 | x2 = 15
0020F593  // and  x11, x1, x2    ; x11 = x1 & x2 = 0

