00A08613  // addi x12, x1, 10    ; x12 = x1 + 10 = 15
00109693  // slli x13, x1, 1     ; x13 = x1 << 1 = 10
0010D713  // srli x14, x1, 1     ; x14 = x1 >> 1 = 2
00C0C793  // xori x15, x1, 12    ; x15 = x1 ^ 12 = 9

// Load Upper Immediate
12345837  // lui  x16, 0x12345   ; x16 = 0x12345000
ABCDE8B7  // lui  x17, 0xABCDE   ; x17 = 0xABCDE000

// Store operations (store to data memory)
00102023  // sw   x1, 0(x0)      ; mem[0] = x1 = 5
00202223  // sw   x2, 4(x0)      ; mem[4] = x2 = 10
00302423  // sw   x3, 8(x0)      ; mem[8] = x3 = 15

